        $('#countEstimateId').text('#1000' + EstimateId);

        var EstimateDetails = '';
        getEstimateById(EstimateId)
        function getEstimateById(EstimateId) {
            $.ajax({
                url: 'https://localhost:44367/api/Estimates/' + EstimateId,
                type: 'GET',
                dataType: 'json',
                success: function (data) {

                    EstimateDetails = data;
                    var fullDateTime = EstimateDetails.DateCreated;
                    var datePart = fullDateTime.split(' ')[0];
                    $('#dateEstimateCreated').text(datePart);
                    $('#dataEstimateName').text(EstimateDetails.Name);
                    console.log()
                },
                error: function (error) {
                    console.error('Error fetching customer data:', error);

                }
            });
        }

        fetchCustomerData(CustomerId);
        function fetchCustomerData(customerId) {
            $.ajax({
                url: 'https://localhost:44367/api/customers/' + customerId,
                type: 'GET',
                dataType: 'json',
                success: function (data) {

                    populateTableRow(data);
                    console.log(data)
                },
                error: function (error) {
                    console.error('Error fetching customer data:', error);

                }
            });
        }

        function populateTableRow(customerData) {

            $('#name').html(` ${customerData.FirstName}`);
            $('#nameInput').val(`${customerData.FirstName}`);
            $('#salepersonInput').val(`${customerData.Salesman}`);
            $('#salesPerson').html(` ${customerData.Salesman}`);
            $('#setOfficeLoactionValue').val($('#officeLoactionValue').text());
        }


        $(document).ready(function () {
            $("#estimateButton").click(function () {
                // Show the modal
                $('#myModal').modal('show');
