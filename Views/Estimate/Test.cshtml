            // Attach a click event to the modal close button
            $("#close-modal").click(function () {
                // Close the modal
                $('#myModal').modal('hide');
            });
        });

        //post the estimate
        document.getElementById('postDataForm').addEventListener('submit', function (e) {
            e.preventDefault();

            // Get form data
            const formData = {
                "UserId": EstimateDetails.UserId,
                "LeadsId": EstimateDetails.LeadsId,
                "Name": document.getElementById('estimateName').value,

                "Notes": document.getElementById('notes').value,

                "Type": document.getElementById('type').value,

                "HidePhaseTotal": document.getElementById('hidePhaseTotal').checked,
                "DisplayDiscountAmountOnPrintable": document.getElementById('displayDiscountAmountOnPrintable').checked,
                "HideEstimateTotal": document.getElementById('hideEstimateTotal').checked,
                "DepositAmount": parseFloat(document.getElementById('depositAmount').value),

            };


            // Make AJAX request
            fetch('https://localhost:44367/api/Estimates', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',f
                },
                body: JSON.stringify(formData),
            })
                .then(response => {
                    console.log('Raw Response:', response);
                    return response.json();
                })
                .then(data => {
                    console.log('Successfully posted:', data);

                    // Check if the server response contains EstimateId
                    if (data) {
                        EstimateId = data.Result
                        var testPageUrl = '/Estimate/Test?CustomerId=' + CustomerId + '&EstimateId=' + EstimateId;
                        window.location.href = testPageUrl;

                    } else {
                        console.log('EstimateId not found in the response:', data);
                    }


                    // Close the modal if needed
                    document.getElementById('myModal').style.display = 'none';
                })
                .catch((error) => {
                    console.error('Error:', error);
                });




        });

