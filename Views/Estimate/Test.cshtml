                        console.log('Location updated successfully:', response);
                        $('#officeLoactionValue').text(selectedOption);
                      //  $('#EditEstimateLocation').modal('hide');
                    },
                    error: function (error) {
                        console.error('Error updating location:', error);
                    }
                });
            } else {
                console.warn('Please select a valid option before saving.');
            }
        });

        $("#EditEstimateLocation .dropdown-menu a").click(function () {
            $("#SelectLocationButton").text($(this).text());
            $("#SelectLocationButton").attr("data-selected", $(this).text());
        });

    </script>
</html>